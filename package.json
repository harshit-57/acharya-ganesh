{
    "name": "acharya-ganesh",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "node server",
        "build": "npm run build:client && npm run build:server",
        "build:client": "vite build --outDir dist/client",
        "build:server": "vite build --ssr src/entry-server.jsx --outDir dist/server",
        "preview": "npm run build:client && npm run build:server && cross-env NODE_ENV=production node server",
        "postbuild": "react-snap"
    },
    "reactSnap": {
        "inlineCss": true,
        "puppeteerArgs": [
            "--no-sandbox"
        ],
        "source": "dist/client",
        "include": [
            "/",
            "/about-us",
            "/contact",
            "/blogs",
            "/services",
            "/spirituality",
            "/course",
            "/web-stories",
            "/blog/:category",
            "/:category/:slug",
            "/spirituality/category/:category"
        ]
    },
    "dependencies": {
        "@emotion/react": "^11.10.6",
        "@emotion/styled": "^11.10.6",
        "@loadable/component": "^5.16.7",
        "@mui/icons-material": "^5.11.11",
        "@mui/material": "^5.11.13",
        "@mui/x-date-pickers": "^7.26.0",
        "@n8tb1t/use-scroll-position": "^2.0.3",
        "@xeger/quill-image-actions": "^0.7.2",
        "@xeger/quill-image-formats": "^0.7.2",
        "axios": "^1.7.9",
        "compression": "^1.7.5",
        "date-fns": "^4.1.0",
        "dayjs": "^1.11.13",
        "echarts": "^5.6.0",
        "echarts-for-react": "^3.0.2",
        "express": "^5.0.1",
        "filepond": "^4.32.7",
        "filepond-plugin-image-preview": "^4.6.12",
        "html-react-parser": "^5.2.2",
        "html-to-text": "^9.0.5",
        "jwt-decode": "^4.0.0",
        "lodash": "^4.17.21",
        "moment": "^2.30.1",
        "quill-image-resize-module-react": "^3.0.0",
        "react": "^18.3.1",
        "react-datepicker": "^8.0.0",
        "react-dom": "^18.3.1",
        "react-filepond": "^7.1.3",
        "react-helmet-async": "^2.0.5",
        "react-insta-stories": "^2.7.0",
        "react-perfect-scrollbar": "^1.5.8",
        "react-quill": "^2.0.0",
        "react-router-dom": "^7.1.1",
        "react-snap": "^1.23.0",
        "react-toastify": "^11.0.3",
        "sirv": "^3.0.0",
        "use-breakpoint": "^4.0.5",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "@types/react-dom": "^18.3.1",
        "@vitejs/plugin-legacy": "^6.1.1",
        "@vitejs/plugin-react": "^4.3.4",
        "cross-env": "^7.0.3",
        "vite": "^6.0.1"
    }
}